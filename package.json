  },
  "devDependencies": {
    "spago": "^0.93.27"
  },
  "author": "id3as",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/id3as/purescript-backend-erl.git"
  },
  "bugs": {
    "url": "https://github.com/id3as/purescript-backend-erl/issues"
  },
  "homepage": "https://github.com/id3as/purescript-backend-erl#readme",
  "keywords": [
    "PureScript",
    "backend"
  ]
}
