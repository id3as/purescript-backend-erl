{
  "name": "purs-backend-erl",
  "version": "0.0.2",
  "description": "PureScript to Erlang optimizing compiler, based on purescript-backend-optimizer.",
  "main": "index.js",
  "type": "module",
  "files": [
    "LICENSE",
    "index.js",
    "bundle/index.js"
  ],
  "bin": {
    "purs-backend-erl": "./index.js"
  },
  "scripts": {
    "test": "spago test -- --run",
